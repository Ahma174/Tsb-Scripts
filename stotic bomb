-- Access Roblox's Lighting service
local lighting = game:GetService("Lighting")

-- Set lighting properties
lighting.Brightness = 2 -- Increase brightness for a sharper feel
lighting.OutdoorAmbient = Color3.fromRGB(70, 70, 70) -- Gives a slightly muted tone
lighting.FogEnd = 2000 -- Adjust for depth in the background
lighting.FogColor = Color3.fromRGB(255, 255, 255) -- Create a bright horizon
lighting.GlobalShadows = true -- Enable shadows for depth
lighting.OutdoorAmbient = Color3.fromRGB(40, 40, 40) -- Darken the outdoor ambient to emphasize shadows

-- Add a bloom effect for a glowing environment
local bloom = Instance.new("BloomEffect", lighting)
bloom.Intensity = 1
bloom.Size = 24
bloom.Threshold = 1 -- Lowering this will create a stronger bloom

-- Add atmosphere for depth and color shift
local atmosphere = Instance.new("Atmosphere", lighting)
atmosphere.Density = 0.3
atmosphere.Offset = 0.25
atmosphere.Color = Color3.fromRGB(199, 204, 255) -- Sky blue tint
atmosphere.Decay = Color3.fromRGB(104, 112, 171) -- Fade to darker blue
atmosphere.Glare = 0.2
atmosphere.Haze = 1


