local Lighting = game.Lighting

-- Set initial time to day
Lighting.TimeOfDay = "12:00:00" -- Start at noon

-- Duration for the transition (in seconds)
local duration = 0.1

-- Number of steps for the transition
local steps = 30

-- Calculate the step size
local stepSize = duration / steps

for i = 0, steps do
    -- Calculate the current time in hours
    local currentHour = 12 + (12 * (i / steps)) -- Smoothly go from 12 (noon) to 24 (midnight)
    
    -- Set the TimeOfDay
    Lighting.TimeOfDay = string.format("%02d:00:00", currentHour)
    
    -- Wait before the next step
    wait(stepSize)
end


